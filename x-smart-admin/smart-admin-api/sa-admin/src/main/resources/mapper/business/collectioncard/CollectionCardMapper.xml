<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.lab1024.sa.admin.module.business.collectioncard.dao.CollectionCardDao">

    <!-- 分页查询 -->
    <select id="queryPage" resultType="net.lab1024.sa.admin.module.business.collectioncard.domain.vo.CollectionCardVO">
        SELECT
        *
        FROM t_collection_card
    </select>

    <select id="getAllOptions" resultType="net.lab1024.sa.admin.module.business.collectioncard.domain.vo.CollectionCardOptionsVO">
        SELECT
            id, bank_name, card_no, account_name
        FROM t_collection_card
        WHERE delete_flag = 0
    </select>


</mapper>